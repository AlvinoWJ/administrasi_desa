	<queryString language="SQL">
		<![CDATA[SELECT 
    s.id_surat,
    s.nama,
    s.nik,
    s.tempat_tanggal_lahir,
    s.alamat,
    s.jenis_surat,
    s.tanggal_buat,
    s.statusSurat,
    s.nomor_surat,
    d.jenis_kelamin,
    d.pekerjaan,
    d.agama
FROM 
    surat s
JOIN 
    surat_domisili d ON s.id_surat = d.id_surat
WHERE 
    s.jenis_surat = 'Surat Keterangan Domisili';]]>
